{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "fridge",
  "mainBinaryName": "fridge",
  "identifier": "com.innsbluck.fridge",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5174",
    "beforeDevCommand": "pnpm vite:dev",
    "beforeBuildCommand": "pnpm prettier && pnpm vite:build"
  },
  "app": {
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["*/**"]
      }
    }
  },
  "plugins": {
    "PreventOverflow": true
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "resources": ["resources/**/*", "icons/**/*"],
    "active": true,
    "icon": ["icons/icon.ico", "icons/icon.icns", "icons/icon.png", "icons/32x32.png", "icons/64x64.png", "icons/128x128.png", "icons/128x128@2x.png"]
  }
}
